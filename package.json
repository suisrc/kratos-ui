{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "plugin": "umi plugin list"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.4.1",
    "@ant-design/pro-table": "^2.5.4",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.7",
    "@types/history": "^4.7.7",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.159",
    "@types/numeral": "^0.0.28",
    "@types/qs": "^6.9.4",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@umijs/preset-react": "^1.5.20",
    "@umijs/test": "^3.2.14",
    "antd-img-crop": "^3.9.0",
    "classnames": "^2.2.6",
    "lint-staged": "^10.0.7",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.2",
    "prettier": "^1.19.1",
    "pro-layout": "^0.0.7",
    "qs": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.14",
    "umi-request": "^1.0.0",
    "use-merge-value": "^1.0.1",
    "yorkie": "^2.0.0"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "README.*.md",
      "docker",
      "CNAME",
      "create-umi",
      "doc"
    ]
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.3",
    "mockjs": "^1.1.0"
  }
}
